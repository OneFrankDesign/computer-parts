{
  "name": "computer-parts",
  "version": "1.0.0",
  "description": "an educational repo to learn about computer architecture",
  "homepage": "https://github.com/OneFrankDesign/computer-parts#readme",
  "bugs": {
    "url": "https://github.com/OneFrankDesign/computer-parts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OneFrankDesign/computer-parts.git"
  },
  "license": "ISC",
  "author": "Frank Krogh",
  "type": "module",
  "main": "index.js",
  "imports": {
    "#types/*": {
      "types": "./src/types/*.ts",
      "default": "./dist/types/*.js"
    },
    "#logic_gates/*": {
      "types": "./src/logic_gates/elementary/*/index.ts",
      "default": "./dist/logic_gates/elementary/*/index.js"
    },
    "#composite_gates/*": {
      "types": "./src/logic_gates/composite/*/index.ts",
      "default": "./dist/logic_gates/composite/*/index.js"
    }
  },
  "scripts": {
    "compile": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
